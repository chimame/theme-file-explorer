{
  "name": "@nosferatu500/theme-file-explorer",
  "version": "3.0.1",
  "description": "File explorer theme for react-sortable-tree",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production TARGET=umd webpack --bail",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production TARGET=demo webpack --bail",
    "clean": "rimraf dist",
    "clean:demo": "rimraf build",
    "start": "cross-env NODE_ENV=development TARGET=development webpack-dev-server --inline --hot",
    "lint": "eslint .",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"**/*.{js,jsx,css,scss}\"",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest",
    "dev": "cross-env NODE_ENV=development TARGET=demo webpack serve --mode development --env development"
  },
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/frontend-collective/@nosferatu500/theme-file-explorer"
  },
  "homepage": "https://github.com/frontend-collective/@nosferatu500/theme-file-explorer",
  "bugs": "https://github.com/frontend-collective/@nosferatu500/theme-file-explorer/issues",
  "contributors": [
    "Chris Fritz",
    "Vitalii Shvetsov"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-dnd": "11.1.3"
  },
  "peerDependencies": {
    "@nosferatu500/react-sortable-tree": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@nosferatu500/react-sortable-tree": "^3.0.0",
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.2.0",
    "json-loader": "^0.5.7",
    "node-sass": "^6.0.0",
    "postcss-loader": "^6.1.0",
    "prettier": "^1.19.1",
    "react": "^17.0.2",
    "react-addons-shallow-compare": "^15.6.3",
    "react-dnd-test-backend": "^2.6.0",
    "react-dnd-touch-backend": "^0.3.21",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^2.7.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [
    "react",
    "react-component"
  ]
}